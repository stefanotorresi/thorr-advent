{
    "name": "stefanotorresi/thorr-advent",
    "description": "Advent of Code puzzle solutions wrapped in a PHP7 CLI app",
    "type": "Puzzles",
    "require": {
        "php": "^7.0",
        "symfony/console": "^3.0",
        "beberlei/assert": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Thorr\\Advent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Thorr\\Advent\\Test\\": "test/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "bin": ["advent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stefano Torresi",
            "email": "stefano@torresi.io"
        }
    ]
}
